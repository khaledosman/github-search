{
  "name": "github-search",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "echo 'ADD YOUR GITHUB TOKEN TO src/environments/environment.ts' && ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "replace-variables": "npx ngssc insert src/ --search",
    "deploy:gh-pages": "ng build --prod --output-path docs --base-href /github-search/ && cp docs/index.html docs/404.html && git add . && git commit -m 'deploy app' && git push origin master",
    "deploy:netlify": "npm run-script replace-variables && npx ngssc wrap-aot ng build --prod",
    "invalidate-cache": "aws --profile personal configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id <CLOUDFRONT_DISTRIBUTION_ID> --paths '/*'",
    "deploy": "ng build --prod --es5-browser-support=false && aws --profile personal s3 sync dist/github-search s3://my-github-search.io --delete",
    "deploy:surge": "surge --domain https://serious-squirrel.sh",
    "fetch-schema": "node fetch-schema.js"
  },
  "private": true,
  "husky": {
    "hooks": {
      "post-commit": "tslint --fix --project ."
    }
  },
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/platform-server": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@angular/service-worker": "^9.0.5",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/cli": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@types/node": "^13.9.0",
    "angular-server-side-configuration": "^9.0.2",
    "husky": "^4.2.3",
    "node-fetch": "^2.6.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.5"
  }
}
