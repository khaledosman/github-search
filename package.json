{
  "name": "github-search",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "echo 'ADD YOUR GITHUB TOKEN TO src/environments/environment.ts' && ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "replace-variables": "npx ngssc insert src/ --search",
    "deploy:gh-pages": "ng build --prod --output-path docs --base-href /github-search/ && cp docs/index.html docs/404.html && git add . && git commit -m 'deploy app' && git push origin master",
    "deploy:netlify": "npm run-script replace-variables && npx ngssc wrap-aot ng build --prod",
    "invalidate-cache": "aws --profile personal configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id <CLOUDFRONT_DISTRIBUTION_ID> --paths '/*'",
    "deploy": "ng build --prod --es5-browser-support=false && aws --profile personal s3 sync dist/github-search s3://my-github-search.io --delete",
    "deploy:surge": "surge --domain https://serious-squirrel.sh",
    "fetch-schema": "node fetch-schema.js"
  },
  "private": true,
  "husky": {
    "hooks": {
      "post-commit": "tslint --fix --project ."
    }
  },
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/pwa": "^0.800.6",
    "@angular/router": "^8.0.3",
    "@angular/service-worker": "^8.0.3",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "core-js": "^3.1.4",
    "graphql": "^14.4.1",
    "graphql-tag": "^2.10.1",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@types/node": "~12.0.10",
    "angular-server-side-configuration": "^2.0.0",
    "husky": "^2.7.0",
    "node-fetch": "^2.6.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.2"
  }
}
